import cv2

img = cv2.imread('/home/camel/Desktop/rpiMiniProject/test.jpg')

img_canny = cv2.Canny(img, 50, 150)
cv2.imwrite('/home/camel/Desktop/rpiMiniProject/edit.jpg', img_canny)

cv2.waitKey(0)
cv2.destroyAllWindows()


